<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.the.man.member.model.repository.MemberMapper">
	
	<resultMap type="socialMember" id="socialMemberResultSet">
		<result property="socialId" column="SOCIAL_ID"/>
		<result property="userNickName" column="USER_NICKNAME" />
		<result property="userProfileimage" column="USER_PROFILE" />
	</resultMap>
	
	<select id="selectUser" parameterType="socialMember" resultMap="socialMemberResultSet">
		SELECT
		       SOCIAL_ID,
		       USER_NICKNAME,
		       USER_PROFILE
		  FROM
		       TB_USER
		 WHERE
		       SOCIAL_ID = #{socialId}
	</select>
